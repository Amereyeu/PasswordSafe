<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkBox" id="divider">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkStack" id="_unlocked_db_deck">
        <child>
          <object class="GtkStack" id="_unlocked_db_stack">
            <child>
              <object class="GtkStack" id="list_stack">
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwBin" id="_edit_page_bin"/>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkActionBar" id="action_bar">
        <property name="valign">end</property>
      </object>
    </child>
  </object>
  <object class="GtkStack" id="browser_stack">
    <child>
      <object class="AdwClamp" id="browser_clamp">
        <child>
          <object class="GtkListBox" id="list_box">
            <property name="name">BrowserListBox</property>
            <property name="valign">start</property>
            <property name="selection_mode">none</property>
            <style>
              <class name="content"/>
            </style>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="AdwStatusPage" id="empty_group_box">
        <property name="icon_name">folder-symbolic</property>
        <property name="description" translatable="true">Add elements with the âž• button.</property>
        <property name="title" translatable="true">Empty Group</property>
      </object>
    </child>
  </object>
</interface>
