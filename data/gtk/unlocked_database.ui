<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UnlockedDatabase" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="AdwLeaflet" id="_unlocked_db_deck">
        <property name="can_unfold">False</property>
        <child>
          <object class="GtkBox" id="_main_view">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkStack" id="headerbar_stack"/>
            </child>
            <child>
              <object class="GtkSearchBar" id="search_bar">
                <child>
                  <object class="AdwClamp">
                    <child>
                      <object class="GtkSearchEntry" id="search_entry">
                        <property name="hexpand">True</property>
                        <property name="activates_default">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkStack" id="_unlocked_db_stack">
                <child>
                  <object class="GtkStack" id="_stack">
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwBin" id="_edit_page_bin"/>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkActionBar" id="action_bar">
        <property name="valign">end</property>
      </object>
    </child>
  </template>
</interface>
