<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UnlockedDatabase" parent="AdwBin">
    <child>
      <object class="AdwNavigationView" id="_navigation_view">
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Browser</property>
            <child>
              <object class="AdwToolbarView" id="_main_view">
                <child type="top">
                  <object class="GtkStack" id="headerbar_stack"/>
                </child>
                <child type="top">
                  <object class="GtkSearchBar" id="search_bar">
                    <accessibility>
                      <property name="label" translatable="yes">Search</property>
                    </accessibility>
                    <child>
                      <object class="AdwClamp">
                        <child>
                          <object class="GtkSearchEntry" id="search_entry">
                            <property name="hexpand">True</property>
                            <property name="activates_default">True</property>
                            <property name="placeholder-text" translatable="yes">Search</property>
                            <accessibility>
                              <relation name="labelled-by">search_bar</relation>
                            </accessibility>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkStack" id="_unlocked_db_stack">
                    <child>
                      <object class="GtkStack" id="_stack">
                      </object>
                    </child>
                  </object>
                </child>
                <child type="bottom">
                  <object class="GtkActionBar" id="action_bar"/>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage" id="_edit_page">
            <property name="title" translatable="yes">Edit</property>
            <signal name="hidden" handler="_on_edit_page_hidden" swapped="no"/>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
