<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="UnlockedDatabasePage" parent="AdwBin">
    <property name="vexpand">True</property>
    <property name="child">
      <object class="GtkStack" id="stack">
        <child>
          <object class="GtkScrolledWindow" id="scrolled_window">
            <child>
              <object class="AdwClamp" id="clamp">
                <property name="margin-top">24</property>
                <property name="margin-bottom">24</property>
                <property name="margin-start">12</property>
                <property name="margin-end">12</property>
                <child>
                  <object class="GtkListBox" id="list_box">
                    <property name="selection_mode">none</property>
                    <property name="valign">start</property>
                    <style>
                      <class name="boxed-list"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwStatusPage" id="empty_group_box">
            <property name="icon_name">folder-symbolic</property>
            <property name="description" translatable="yes">Add elements with the âž• button.</property>
            <property name="title" translatable="yes">Empty Group</property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>
